{
  "name": "nest-auth-api",
  "version": "0.0.1",
  "description": "API REST with NestJS, TypeORM and OAuth2",
  "private": true,
  "author": "git.5nmmw@passinbox.com",
  "license": "MIT",
  "type": "module",
  "main": "dist/main.js",
  "bugs": {
    "url": "https://github.com/garbarok/nest-typescript-starter/issues"
  },
  "homepage": "https://github.com/garbarok/nest-typescript-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbarok/nest-typescript-starter.git"
  },
  "keywords": [
    "typescript",
    "nestjs",
    "template"
  ],
  "scripts": {
    "build": "bun --run build:clean && nest build --path tsconfig.build.json",
    "build:clean": "rimraf dist; exit 0",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start swc",
    "start:dev": "nest start swc --watch",
    "start:debug": "nest start swc --debug --watch",
    "start:prod": "bun dist/main",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "lint:yaml": "chmod +x scripts/lint_yaml.sh && ./scripts/lint_yaml.sh",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:file": "eslint --ignore-path .gitignore",
    "typos": "chmod +x scripts/check_typos.sh && ./scripts/check_typos.sh",
    "prepare": "[ -f .husky/install.mjs ] && node .husky/install.mjs || true"
  },
  "engines": {
    "pnpm": ">=9.x",
    "npm": ">=10.0.0",
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.11",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.11",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "execa": "^9.5.2",
    "lint-staged": "^15.5.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.11",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.0.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.35.0",
    "@swc/core-linux-arm64-musl": "^1.11.9",
    "@swc/core-linux-x64-gnu": "^1.11.9",
    "@swc/core-linux-x64-musl": "^1.11.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "trustedDependencies": [
    "@nestjs/core",
    "@swc/core"
  ],
  "packageManager": "pnpm@10.6.3+sha512.bb45e34d50a9a76e858a95837301bfb6bd6d35aea2c5d52094fa497a467c43f5c440103ce2511e9e0a2f89c3d6071baac3358fc68ac6fb75e2ceb3d2736065e6"
}
